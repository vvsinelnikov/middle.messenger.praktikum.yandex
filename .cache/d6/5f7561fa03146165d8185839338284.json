der: 0;\n    border-radius: 28px;\n    background: url('../../../static/images/send-icon.svg') no-repeat center;\n    background-size: cover;\n    transition: opacity .25s;\n    outline: none;\n    -webkit-transform: rotate(180deg);\n    -moz-transform: rotate(180deg);\n    -ms-transform: rotate(180deg);\n    -o-transform: rotate(180deg);\n    transform: rotate(180deg);\n}\n\n.profile__home-button:hover {\n    opacity: 0.8;\n    cursor: pointer;\n}\n\n.profile__right {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    width: 100%;\n}\n\n.profile__avatar {\n    margin: 60px 0 20px 0;\n    width: 130px;\n    height: 130px;\n    border-radius: 130px;\n    background: url('../../../static/images/profile-avatar.svg') no-repeat center;\n    background-size: cover;\n}\n\n.profile__avatar:hover {\n    cursor: pointer;\n}\n\n.profile__avatar:hover::after {\n    content: 'Поменять аватар';\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    width: 130px;\n    height: 130px;\n    border-radius: 130px;\n    box-sizing: border-box;\n    padding: 10px;\n    background-color: #000;\n    color: #fff;\n    font-weight: var(--font-weight-normal);\n    font-size: var(--font-size-normal);\n    line-height: 14px;\n    text-align: center;\n    opacity: .5;\n}\n\n.profile__entry {\n    display: flex;\n    flex-direction: column;\n    width: 510px;\n    margin: 0;\n    padding-top: 10px;\n}\n\n.profile__field {\n    display: flex;\n    flex-direction: row;\n    align-items: end;\n    border-bottom: solid 1px rgba(0, 0, 0, 0.2);\n}\n\n.profile__entry:last-child .profile__field {\n    border-bottom: none;\n}\n\n.profile__entry-name {\n    font-weight: var(--font-weight-normal);\n    font-size: var(--font-size-normal);\n    line-height: 13px;\n    color: var(--black-text);\n    margin: 0;\n    padding: 0 10px 5px 0;\n    white-space: nowrap;\n}\n\n.profile__entry-input {\n    font-weight: var(--font-weight-normal);\n    font-size: var(--font-size-normal);\n    line-height: var(--font-height-normal);\n    text-align: right;\n    margin: 0;\n    padding: 3px;\n    width: 100%;\n    border: none;\n    outline: none;\n}\n\n.profile__entry-input:disabled {\n    background-color: #fff;\n}\n\n.profile__error {\n    display: block;\n    padding-top: 5px;\n    font-size: 8px;\n    line-height: 8px;\n    color: var(--error);\n}\n\n.profile__error_hidden {\n    display: none;\n}\n\n.profile__link {\n    font-weight: var(--font-weight-normal);\n    font-size: var(--font-size-normal);\n    line-height: var(--font-height-normal);\n    color: var(--primary-blue);\n    text-decoration: none;\n    border-top: solid 1px rgba(0, 0, 0, 0.2);\n    padding: 10px 0;\n    transition: opacity .25s;\n}\n\n.profile__link:first-of-type {\n    border: none;\n    margin-top: 50px;\n}\n\n.profile__link:hover {\n    opacity: 0.8;\n    cursor: pointer;\n}\n\n.profile__link_red {\n    color: var(--error);\n}\n\n.profile__button {\n    width: 200px;\n    margin: 30px 0 0 0;\n}"},"lineCount":null}},"error":null,"hash":"e98d50d1a586a5be0942d4df1ecd5602","cacheData":{"env":{}}}